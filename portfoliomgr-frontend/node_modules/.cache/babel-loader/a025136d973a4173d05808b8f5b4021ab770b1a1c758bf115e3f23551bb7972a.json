{"ast":null,"code":"export default {\n  data() {\n    return {\n      UserName: \"\",\n      UserEmail: \"\",\n      UserPswd: \"\",\n      profile: null,\n      ConfPswd: \"\",\n      PswdMisMatch: false,\n      success: false,\n      error: false\n    };\n  },\n  watch: {\n    UserPswd() {\n      this.validatePasswords();\n    },\n    ConfPswd() {\n      this.validatePasswords();\n    }\n  },\n  methods: {\n    onFileChange(event) {\n      this.profile = event.target.files[0];\n    },\n    validatePasswords() {\n      this.PswdMisMatch = this.UserPswd !== this.ConfPswd;\n    },\n    async Signup() {\n      try {\n        const userData = new FormData();\n        userData.append(\"UserName\", this.UserName);\n        userData.append(\"Email\", this.UserEmail);\n        userData.append(\"Password\", this.UserPswd);\n        userData.append(\"Profile\", this.profile);\n        const response = await fetch(\"http://127.0.0.1:5000/api/user\", {\n          method: \"POST\",\n          body: userData\n        });\n        if (response.ok) {\n          this.success = true;\n          this.error = false;\n        } else {\n          this.error = true;\n          this.success = false;\n        }\n      } catch (e) {\n        console.log(e);\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","UserName","UserEmail","UserPswd","profile","ConfPswd","PswdMisMatch","success","error","watch","validatePasswords","methods","onFileChange","event","target","files","Signup","userData","FormData","append","response","fetch","method","body","ok","e","console","log"],"sources":["/Users/pranav/DBMS_Project/Portfolio-Manager/portfoliomgr-frontend/src/components/SignUp.vue"],"sourcesContent":["<template>\n  <div\n    id=\"main\"\n    class=\"container-fluid d-flex justify-content-center align-items-center vh-100\"\n  >\n    <form\n      @submit.prevent=\"Signup\"\n      class=\"container p-5 rounded-lg shadow-lg custom-form\"\n      v-if=\"!success && !error\"\n    >\n      <h2 class=\"text-center mb-4\">User Registration</h2>\n\n      <div class=\"form-group\">\n        <label>Enter User Name:</label>\n        <input v-model=\"UserName\" type=\"text\" class=\"form-control\" required />\n      </div>\n      <div class=\"form-group\">\n        <label>Upload Profile Picture :</label>\n        <input type=\"file\" @change=\"onFileChange\" />\n      </div>\n      <div class=\"form-group\">\n        <label>Enter User Email ID:</label>\n        <input v-model=\"UserEmail\" type=\"email\" class=\"form-control\" required />\n      </div>\n\n      <div class=\"form-group\">\n        <label>Enter Password:</label>\n        <input\n          v-model=\"UserPswd\"\n          type=\"password\"\n          class=\"form-control\"\n          required\n        />\n      </div>\n\n      <div class=\"form-group\">\n        <label>Confirm Password:</label>\n        <input\n          v-model=\"ConfPswd\"\n          type=\"password\"\n          class=\"form-control\"\n          required\n        />\n      </div>\n      <span\n        v-if=\"PswdMisMatch\"\n        style=\"color: red; font-family: 'Courier New', Courier, monospace\"\n      >\n        Passwords Don't Match!\n      </span>\n\n      <button\n        type=\"submit\"\n        class=\"btn btn-primary btn-block\"\n        v-bind:disabled=\"PswdMisMatch\"\n      >\n        Submit\n      </button>\n    </form>\n\n    <div v-else class=\"result-message\">\n      <div v-if=\"success\" class=\"success-message\">\n        <p>USER SUCCESSFULLY CREATED!</p>\n        <router-link to=\"/login\" class=\"btn btn-success btn-sm\"\n          >Login</router-link\n        >\n      </div>\n      <div v-if=\"error\" class=\"error-message\">\n        <p>ERROR CREATING USER!</p>\n        <router-link to=\"/\" class=\"btn btn-danger btn-sm\">Sign Up</router-link>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      UserName: \"\",\n      UserEmail: \"\",\n      UserPswd: \"\",\n      profile: null,\n      ConfPswd: \"\",\n      PswdMisMatch: false,\n      success: false,\n      error: false,\n    };\n  },\n  watch: {\n    UserPswd() {\n      this.validatePasswords();\n    },\n    ConfPswd() {\n      this.validatePasswords();\n    },\n  },\n\n  methods: {\n    onFileChange(event) {\n      this.profile = event.target.files[0];\n    },\n    validatePasswords() {\n      this.PswdMisMatch = this.UserPswd !== this.ConfPswd;\n    },\n    async Signup() {\n      try {\n        const userData = new FormData();\n        userData.append(\"UserName\", this.UserName);\n        userData.append(\"Email\", this.UserEmail);\n        userData.append(\"Password\", this.UserPswd);\n        userData.append(\"Profile\", this.profile);\n\n        const response = await fetch(\"http://127.0.0.1:5000/api/user\", {\n          method: \"POST\",\n          body: userData,\n        });\n\n        if (response.ok) {\n          this.success = true;\n          this.error = false;\n        } else {\n          this.error = true;\n          this.success = false;\n        }\n      } catch (e) {\n        console.log(e);\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n#main {\n  background-image: url(../assets/BG.png);\n  background-size: 100%;\n  background-position: center;\n  background-repeat: no-repeat;\n}\n.custom-form {\n  background-color: #f8f9fa;\n  border-radius: 10px;\n  box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.1);\n  width: 30%;\n}\n\n.custom-form label {\n  font-weight: bold;\n  color: #495057;\n}\n\n.custom-form input {\n  border: 1px solid #ced4da;\n  border-radius: 5px;\n  padding: 8px;\n  margin-bottom: 10px;\n}\n\n.custom-form button {\n  background-color: #007bff;\n  border: none;\n  border-radius: 5px;\n  color: #fff;\n  padding: 10px;\n  cursor: pointer;\n}\n\n.custom-form button:hover {\n  background-color: #0056b3;\n}\n\n.result-message {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  margin-top: 20px;\n}\n\n.success-message,\n.error-message {\n  background-color: #f8f9fa;\n  border: 1px solid;\n  border-radius: 5px;\n  padding: 10px;\n  margin: 10px;\n  text-align: center;\n}\n\n.success-message {\n  border-color: #d4edda;\n  color: #28a745;\n}\n\n.error-message {\n  border-color: #f8d7da;\n  color: #721c24;\n}\n</style>\n"],"mappings":"AA4EA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE,EAAE;MACbC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE,IAAI;MACbC,QAAQ,EAAE,EAAE;MACZC,YAAY,EAAE,KAAK;MACnBC,OAAO,EAAE,KAAK;MACdC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,KAAK,EAAE;IACLN,QAAQA,CAAA,EAAG;MACT,IAAI,CAACO,iBAAiB,CAAC,CAAC;IAC1B,CAAC;IACDL,QAAQA,CAAA,EAAG;MACT,IAAI,CAACK,iBAAiB,CAAC,CAAC;IAC1B;EACF,CAAC;EAEDC,OAAO,EAAE;IACPC,YAAYA,CAACC,KAAK,EAAE;MAClB,IAAI,CAACT,OAAM,GAAIS,KAAK,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;IACtC,CAAC;IACDL,iBAAiBA,CAAA,EAAG;MAClB,IAAI,CAACJ,YAAW,GAAI,IAAI,CAACH,QAAO,KAAM,IAAI,CAACE,QAAQ;IACrD,CAAC;IACD,MAAMW,MAAMA,CAAA,EAAG;MACb,IAAI;QACF,MAAMC,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;QAC/BD,QAAQ,CAACE,MAAM,CAAC,UAAU,EAAE,IAAI,CAAClB,QAAQ,CAAC;QAC1CgB,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAE,IAAI,CAACjB,SAAS,CAAC;QACxCe,QAAQ,CAACE,MAAM,CAAC,UAAU,EAAE,IAAI,CAAChB,QAAQ,CAAC;QAC1Cc,QAAQ,CAACE,MAAM,CAAC,SAAS,EAAE,IAAI,CAACf,OAAO,CAAC;QAExC,MAAMgB,QAAO,GAAI,MAAMC,KAAK,CAAC,gCAAgC,EAAE;UAC7DC,MAAM,EAAE,MAAM;UACdC,IAAI,EAAEN;QACR,CAAC,CAAC;QAEF,IAAIG,QAAQ,CAACI,EAAE,EAAE;UACf,IAAI,CAACjB,OAAM,GAAI,IAAI;UACnB,IAAI,CAACC,KAAI,GAAI,KAAK;QACpB,OAAO;UACL,IAAI,CAACA,KAAI,GAAI,IAAI;UACjB,IAAI,CAACD,OAAM,GAAI,KAAK;QACtB;MACF,EAAE,OAAOkB,CAAC,EAAE;QACVC,OAAO,CAACC,GAAG,CAACF,CAAC,CAAC;MAChB;IACF;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}